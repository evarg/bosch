					<xs:documentation source="2.0.4">now mandatory default: 3415</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="vas" type="offsetEntryType">
				<xs:annotation>
					<xs:documentation source="2.5.1">default: 3425</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="generalAttributeGroup"/>
