	<xs:complexType name="InterfacePanelType">
		<xs:annotation>
			<xs:documentation source="2.15.0"/>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="LogicalNetAddr"/>
		</xs:sequence>
		<xs:attributeGroup ref="netGeneralAttributeGroup"/>
	</xs:complexType>
	<xs:complexType name="P2PType">
		<xs:annotation>
			<xs:documentation source="2.15.0"/>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ipAddress" type="ipAddressType"/>
			<xs:element name="portNumber" type="portNumberType" default="80"/>
			<xs:element name="networkId">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="31"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="crl" type="ipAddressType"/>
