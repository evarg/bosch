				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="OFF"/>
						<xs:enumeration value="RSTP"/>
						<xs:enumeration value="DUAL_HOMING"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="STP_BridgePriority" default="32768">
				<xs:annotation>
					<xs:documentation source="2.8.2">applies only in case redundancyMode=RSTP</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedShort">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="61440"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="STP_HelloTime" default="2">
				<xs:annotation>
					<xs:documentation source="2.8.2">applies only in case redundancyMode=RSTP</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedByte">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="STP_MaxAge" default="20">
				<xs:annotation>
					<xs:documentation source="2.8.2">applies only in case redundancyMode=RSTP</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedByte">
						<xs:minInclusive value="6"/>
						<xs:maxInclusive value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="STP_ForwardDelay" default="15">
				<xs:annotation>
					<xs:documentation source="2.8.2">applies only in case redundancyMode=RSTP</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedByte">
						<xs:minInclusive value="4"/>
						<xs:maxInclusive value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
