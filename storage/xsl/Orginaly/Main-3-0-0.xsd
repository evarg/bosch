	<xs:simpleType name="logicalNodeIDType">
		<xs:annotation>
			<xs:documentation source="2.0.0">composed of 1-3 digits for netGroup a dot and 1-3 digits for netNode (e.g. 3.40)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="messageFilterType">
		<xs:annotation>
			<xs:documentation source="2.0.0">not yet defined</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="netCRCType">
		<xs:annotation>
			<xs:documentation>CRC as decimal unsigned short</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="xs:unsignedShort">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:length value="0"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
