					<xs:simpleType>
						<xs:restriction base="xs:NMTOKEN">
							<xs:enumeration value="BPR_ALL"/>
							<xs:enumeration value="BPR_SINGLE"/>
							<xs:enumeration value="BPR_GROUP"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="activateFireExt" type="yesnoType" default="ST_YES" minOccurs="0" maxOccurs="1"/>
				<xs:element name="allowDetectorRemoval" type="yesnoType" default="ST_NO" minOccurs="0" maxOccurs="1"/>
			</xs:sequence>
			<xs:attributeGroup ref="netGeneralAttributeGroup"/>
		</xs:complexType>
	</xs:element>
