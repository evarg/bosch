			<xs:documentation source="2.0.0">may contain netSIIDType or empty string if not yet resolved</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="globalSIIDType">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:length value="0"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:simpleType name="netLineNumberType">
		<xs:annotation>
			<xs:documentation source="2.0.0">unsigned short, 0 = unused</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:unsignedShort"/>
	</xs:simpleType>
	<xs:simpleType name="netLineTypeType">
		<xs:annotation>
			<xs:documentation source="2.0.0">CAN or ETHERNET</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="CAN"/>
