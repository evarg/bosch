	<xs:complexType name="CAN_PARAMType">
		<xs:sequence>
			<xs:element name="bitRate" type="xs:unsignedLong" default="41666"/>
			<xs:element name="propagationSegment" type="xs:unsignedByte" default="11"/>
			<xs:element name="phaseSegment1" type="xs:unsignedByte" default="2"/>
			<xs:element name="phaseSegment2" type="xs:unsignedByte" default="2"/>
