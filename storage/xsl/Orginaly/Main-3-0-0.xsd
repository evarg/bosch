	<xs:element name="ASSIGNED_MTS_NODES">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="MTS_NODE"/>
			</xs:sequence>
			<xs:attributeGroup ref="netGeneralAttributeGroup"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ASSIGNED_OPCSVRS">
		<xs:annotation>
			<xs:documentation source="2.0.2">OPC-Servers can be assigned to a node of type FPA5000 and FPA5000_RK.
At the moment only one OPC-Server may be assigned to a node.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="OPCSERVER"/>
			</xs:sequence>
			<xs:attributeGroup ref="netGeneralAttributeGroup"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="CAN_PARAM" type="CAN_PARAMType"/>
	<xs:element name="REMOTE_SERVICES_NETWORK_DATA" type="RemoteServicesNetworkDataType"/>
	<xs:element name="CONFIG_DATA">
		<xs:annotation>
			<xs:documentation source="2.0.0">Contains the location of the XML-Configuration of the node and all data needed by RPS to open the local configuration data.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice minOccurs="0">
					<xs:annotation>
						<xs:documentation source="2.0.0">Dual interface: Two alternative ways of including the local configuration of a panel. </xs:documentation>
					</xs:annotation>
					<xs:element name="xmlLocation" type="xs:string">
						<xs:annotation>
							<xs:documentation source="2.0.0">Path of the configuration xml file containing the local node configuration. This shall be the XML-tree of the V1.x.x structure starting at the element 'LOCAL_CONFIGURATION'. If the element is empty the node does not contain a local configuration yet.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="LOCAL_CONFIGURATION"/>
				</xs:choice>
				<xs:element ref="RK_DATA" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="netGeneralAttributeGroup"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="CONFIG_NETWORK">
		<xs:annotation>
			<xs:documentation source="2.5.0">Copied from 2.1.0 revision 1.4</xs:documentation>
			<xs:documentation source="2.6.0">Copied from 2.5.0 revision 1.5</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HEADER">
					<xs:annotation>
						<xs:documentation source="2.0.0">As in the V 1.x.x configurations</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="NETWORK_INFO">
					<xs:annotation>
						<xs:documentation source="2.0.0">Information regarding the network itself like name, description etc. which is not yet defined in the existing local panel configurations but must be present in each panel.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="CUSTOMER_INFO"/>
				<xs:element ref="SUPPORT_INFO"/>
				<xs:element ref="REMOTE_SERVICES_NETWORK_DATA" minOccurs="0">
					<xs:annotation>
						<xs:documentation source="2.13.0">PRELIM optional</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="LZ_STATE_MAPPING" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Must not be inserted by conversion, is inserted by RPS  if neccessary</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="NODES">
					<xs:annotation>
						<xs:documentation source="2.0.0">All nodes which are participants in a network. This can be panels or other devices with a logical netID. External modules are not considered as nodes.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="SWITCHES" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="netGeneralAttributeGroup"/>
			<xs:attribute name="panelEncoding" type="encodingType" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="CONNECTABLE">
		<xs:annotation>
			<xs:documentation source="2.0.1">Is only present if Node is of type RemoteKeypad</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element name="CONNECTABLE_PANEL">
					<xs:complexType>
						<xs:group ref="LogicalNetAddr"/>
						<xs:attributeGroup ref="generalAttributeGroup"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attributeGroup ref="netGeneralAttributeGroup"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="CUSTOMER_INFO" type="CUSTOMER_INFOType">
		<xs:annotation>
			<xs:documentation source="2.0.0">Structure as  in the V 1.x.x configurations. The data will be copied to the local configurations and loaded to each panel, but it has the same contents for the whole network and shall be entered in the network part of RPS.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ETHERNET_PARAM" type="ETHERNET_PARAMType"/>
	<xs:element name="EXPORTS">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="netCRC" minOccurs="0"/>
				<xs:element ref="REPLICATED_SIS" minOccurs="0">
					<xs:annotation>
						<xs:documentation source="2.0.0">All SIIDs which are replicated to one or more remote nodes. This list is filled whenever an SIID is marked as replicated in the local panel configuration.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="REPLICATED_COUNTERS" minOccurs="0">
					<xs:annotation>
						<xs:documentation source="2.0.0">All Counters, which are replicated to one or more remote nodes.  This list is filled whenever a counter is marked as replicated in the local panel configuration.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attributeGroup ref="netGeneralAttributeGroup"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="IMPORTS">
		<xs:annotation>
			<xs:documentation source="2.0.0">The network part of the local panel configuration is created from this import lists. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="netCRC" minOccurs="0"/>
				<xs:element ref="USED_SIS" minOccurs="0"/>
				<xs:element ref="USED_COUNTERS" minOccurs="0"/>
				<xs:element ref="USED_OFFSET_TABLES" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="netGeneralAttributeGroup"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="LOCAL_CONFIGURATION">
		<xs:annotation>
			<xs:documentation source="2.0.0">Starting point for the lcocal configuration subtree or root for standalone local configurations.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="LOCAL_CONFIGURATIONType">
					<xs:attribute name="panelEncoding" type="encodingType" use="optional"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="LZ_STATE_MAPPING">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CONFIGURED_MAPPINGS"/>
			</xs:sequence>
			<xs:attributeGroup ref="netGeneralAttributeGroup"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="MTS_NODE">
		<xs:complexType>
			<xs:group ref="LogicalNetAddr"/>
			<xs:attributeGroup ref="netGeneralAttributeGroup"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="NET_GROUP" type="NET_GROUPType"/>
	<xs:element name="NET_GROUPS">
		<xs:complexType>
			<xs:sequence maxOccurs="255">
				<xs:element ref="NET_GROUP"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="NET_INTERFACE">
		<xs:annotation>
			<xs:documentation source="2.0.4">messagefilter is dropped, CAN_PARAM is added in V2.0.4</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="NET_INTERFACEType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="NET_INTERFACES">
		<xs:annotation>
			<xs:documentation source="2.0.0">Physical interfaces of a node</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="NET_INTERFACE"/>
			</xs:sequence>
			<xs:attributeGroup ref="netGeneralAttributeGroup"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="netCRC" type="netCRCType">
		<xs:annotation>
			<xs:documentation source="2.0.0">CRC16 as decimal unsigned short</xs:documentation>
		</xs:annotation>
	</xs:element>
