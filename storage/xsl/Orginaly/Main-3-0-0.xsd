		<xs:annotation>
			<xs:documentation source="2.0.0">0 -255, 0 = unused</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:unsignedByte">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="255"/>
		</xs:restriction>
