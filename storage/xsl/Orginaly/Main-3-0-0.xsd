			<xs:documentation source="2.0.0">composed like 'netGroup.netNode.SI_Type.number.subnumber' e.g. 1.2.MP_SI_POINT.100.1</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{1,3}\.[0-9]{1,3}\.MP_SI_[A-Z_]+\.[0-9]{1,5}\.[0-9]{1,3}"/>
		</xs:restriction>
	</xs:simpleType>
