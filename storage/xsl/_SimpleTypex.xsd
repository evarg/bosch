		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{1,3}\.[0-9]{1,3}\.MP_SI_[A-Z_]+\.[0-9]{1,5}\.[0-9]{1,3}"/>
		</xs:restriction>
